<?xml version="1.0" encoding="utf-8"?>
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <DigillectBuildToolsVersion>2.0.0</DigillectBuildToolsVersion>
    <Configuration Condition="'$(Configuration)' == ''">Debug</Configuration>
    <NuGetExecutable Condition="'$(NuGetExecutable)' == ''">nuget.exe</NuGetExecutable>
    <NuGetSourcesArgument Condition="'$(NuGetSources)' != ''">-Source "$(NuGetSources)"</NuGetSourcesArgument>
    <NuGetVerbosityArgument Condition="'$(NuGetVerbosity)' != ''">-Verbosity $(NuGetVerbosity)</NuGetVerbosityArgument>
  </PropertyGroup>

  <UsingTask AssemblyFile="$(MSBuildThisFileDirectory)Digillect.Build.Tasks.dll" TaskName="Digillect.Build.Tasks.FileVersionInfo" />
</Project>